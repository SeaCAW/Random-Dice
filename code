#include <iostream>
#include <cmath>
#include <string>
#include <cstdlib>
#include <ctime>
using namespace std;
int main() {
    srand(time(0));
    const int maxV = 6;
    const int minV = 1;
    short f = (rand()%(maxV - minV +1))+ minV;
    short s = (rand()%(maxV - minV +1))+ minV;
    cout << f << " ," << s;
    return 0;
}
